    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "~2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.7",
    "svgstore-cli": "^2.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.16.0",
    "vitest-svelte-kit": "^0.0.6"
  }
